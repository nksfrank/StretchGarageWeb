<body ng-controller="AppController">
    <div id="message" ng-click="close('message')" class="alert" ng-show="Messages">
        <ul>
            <li ng-repeat="m in Messages">
                <div>{{m.Message}}</div>
            </li>
        </ul>
    </div>
    <div class="container">
        @RenderBody()
    </div>
    <div class="footer">
        <p>G. Hedman, J. Takter, N. Frank</p>
        <p>© Stretch Garage</p>
